---
import "../../styles/global.css";
import CustomerApp from "../../components/customer/main";

export const prerender = true;

export function getStaticPaths() {
  return [{ params: { all: undefined } }];
}
---

<!doctype html>
<html lang="cs">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="robots" content="noindex, nofollow" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <title>Můj účet | Samo Fujera</title>
  </head>
  <body class="min-h-screen bg-[var(--background)] text-[var(--foreground)] font-sans antialiased">
    <CustomerApp client:only="react" />
  </body>
</html>
